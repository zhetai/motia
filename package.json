{
  "name": "wistro-monorepo",
  "private": true,
  "scripts": {
    "dev:docs": "pnpm -r --filter=docs start",
    "dev": "pnpm run build && pnpm run -r --parallel dev",
    "build": "pnpm --filter=!docs run -r build",
    "build-config": "node packages/wistro/src/cli/WistroConfigBuilder.js playground",
    "test:unit": "pnpm vitest",
    "test": "concurrently \"pnpm run test:unit\" \"pnpm run test:e2e\"",
    "clean": "rm -rf node_modules pnpm-lock.yaml dist .turbo .next",
    "wistro": "node packages/wistro/src/cli/index.js"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "ignore": "^6.0.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "redis": "^4.7.0",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "volta": {
    "node": "20.9.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
